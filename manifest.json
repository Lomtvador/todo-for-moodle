{
    "author": "Ilya Shabalin",
    "manifest_version": 2,
    "version": "1.0.0",
    "name": "Todo list for moodle.nntc.nnov.ru",
    "description": "Adds todo list on moodle.nntc.nnov.ru",
    "content_scripts": [
        {
            "matches": ["https://moodle.nntc.nnov.ru/*"],
            "js": ["todo.js"]
        }
    ],
    "permissions": ["storage"]
}
